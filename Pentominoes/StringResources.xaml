<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="invalidRangeValue">Please enter a value in the range: {0} to {1}</system:String>
    <system:String x:Key="solutionNumberFormat">Solution: {0} of {1}</system:String>
    <system:String x:Key="noSolutions">-- NO SOLUTIONS --</system:String>
    <system:String x:Key="notYetSolved">Solution: --- of ---</system:String>
    <system:String x:Key="puzzleNumberFormat">Puzzle: {0} of {1}</system:String>
    <system:String x:Key="statusReady">Ready</system:String>
    <system:String x:Key="statusSolving">SOLVING...</system:String>
</ResourceDictionary>